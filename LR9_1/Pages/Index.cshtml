@page
@model List<LR9_1.Models.ObjectModel>
@{
    ViewData["Title"] = "Objects";
}

<h1>@ViewData["Title"]</h1>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>@obj.ID</td>
                <td>@obj.Name</td>
                <td>@obj.Price</td>
            </tr>
        }
    </tbody>
</table>
